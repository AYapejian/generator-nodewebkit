{
    "main": "app/index.html",
    "name": "<%= _.slugify(projectName) %>",
    "description": "<%= projectDescription %>",
    "version": "0.0.1",
    "repository" : { 
        "type" : "git",
        "url" : "http://github.com/<%= gitUser %>/<%= _.slugify(projectName) %>.git"
    },
    "keywords": [
        "node-webkit"
    ],
    "window": {
        "title": "<%= projectName %>",
        "icon": "app/imgs/window_icon.png****NOT_IMPLEMENTED*****",
        "toolbar": true,
        "frame": true,
        "width": <%= windowWidth %>,
        "height": <%= windowHeight %>,
        "position": "mouse",
        "min_width": 100,
        "min_height": 100,
        "max_width": 1080,
        "max_height": 1920
    },
    "webkit": {
        "plugin": true
    },
    "dependencies": {
        "grunt": "~0.4.1"
    }
}
